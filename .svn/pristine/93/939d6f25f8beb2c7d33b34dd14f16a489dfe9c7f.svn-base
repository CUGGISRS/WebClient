    function bind_typeEvent() {
      $('.memus')
        .find('li')
        .bind('click', function () {
          $('.memus').find('li').removeClass('colorgreens'); // 初始化选中样式
          $(this).addClass('colorgreens'); // 被点击的li添加选中样式
          type_text = $(this).text();
          //农产品类型
          type = $(this).attr('id');
          queryDatas();
          chart.setTitle({
            text: '', //location_text + type_text, // 设置主级标题
            x: -20,
          });
        });
    }

    function queryDatas() {
      //获取一种类型的全部名称和价格
      $.ajax({
        url: InterfaceDZSW + '/price/getPriceListByCategory',
        type: 'get',
        dataType: 'json',
        data: { category: type },
        success: function (data) {
          /*if (data.sendDate != undefined) {
                  $("#jghq").html(data.sendDate + "  最新农畜农产品价格行情 ");
              } else {
              }*/
          $('#jghq').html('最新农畜农产品价格行情 ');
          var categories = [];
          var data1 = [];
          for (let i = 0; i < data.data.length; i++) {
            categories.push(data.data[i].productName);
            data1.push(data.data[i].price);
          }
          // categories = data.products.type;
          // data1 = data.products.price1;
          init_data(categories, data1);
        },
        error: function (e) {
          console.log(e);
        },
      });
    }

    function init_data(categories, data1) {
      chart = new Highcharts.Chart({
        chart: {
          renderTo: 'main_con1',
          defaultSeriesType: 'column', // 图表类别，可取值有：line、spline、area、areaspline、bar、column等
          marginRight: 130,
          marginBottom: 50,
          height: 320,
        },
        title: {
          text: '', //location_text + type_text, // 设置一级标题
          x: -20,
          // center
        },
        /*
         * subtitle: { text: '（银川)', //设置二级标题 x: -20 },
         */
        xAxis: {
          categories: categories,

          // 设置x轴的标题
        },
        yAxis: {
          title: {
            text: '', // 设置y轴的标题
          },
          plotLines: [
            {
              value: 0,
              width: 1,
              color: '#fff',
            },
          ],
        },
        tooltip: {
          formatter: function () {
            return '<b>' + this.series.name + '</b><br/>' + this.x + ': ' + this.y + '元/斤'; // 鼠标放在数据点的显示信息，但是当设置显示了每个节点的数据项的值时就不会再有这个显示信息
          },
        },
        legend: {
          layout: 'vertical',
          align: 'right', // 设置说明文字的文字 left/right/top/
          verticalAlign: 'top',
          x: -10,
          y: 100,
          borderWidth: 0,
        },
        plotOptions: {
          column: {
            dataLabels: {
              overflow: 'visible',
              crop: true,
              enabled: true, // dataLabels设为true
              style: {
                color: '#686868',
                fontSize: '8px !important',
                boxShadow: 'none',
                textShadow: 'none',
              },
            },
          },
        },
        animation: true,
        series: [
          {
            name: '最新价格', // 每条线的名称
            data: data1, //每条线的数据
            color: '#ffc346',
          },
        ],
      });
    }

