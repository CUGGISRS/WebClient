      //获取所有农产品类型
      $.ajax({
        url: InterfaceDZSW + '/dic/getDicByType',
        type: 'post',
        dataType: 'json',
        data: { type: '农产品分类', isCodeAsc: true },
        async: false,
        success: function (res) {
          $('#types').empty();

          var length = res.data.length;

          var html = '';
          for (var i = 0; i < res.data.rows.length; i++) {
            if (i == 0) {
              html =
