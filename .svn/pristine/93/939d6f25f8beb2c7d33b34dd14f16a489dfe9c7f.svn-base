    $(document).ready(function () {
      queryCurexpert();
      var timejg = 3000; //轮播间隔时间
      var size = $('.box_img ul li').size();

      $('.box_img ul li').eq(0).show();
      $('.box_tab a').eq(0).addClass('active');
      $('.box_tab a').mouseover(function () {
        $(this).addClass('active').siblings().removeClass('active');
        var index = $(this).index();
        i = index;
        $('.box_img ul li').eq(index).stop().fadeIn(300).siblings().stop().fadeOut(300);
      });

      var i = 0;
      var time = setInterval(move, timejg);

      function move() {
        i++;
        if (i == size) {
          i = 0;
        }

        $('.box_tab a').eq(i).addClass('active').siblings().removeClass('active');
        $('.box_img ul li').eq(i).fadeIn(300).siblings().fadeOut(300);
      }

      $('.box').hover(
        function () {
          clearInterval(time);
        },
        function () {
          time = setInterval(move, timejg);
        }
      );
    });
    //供求-供应信息
    $(function () {
      supplyInfo(0);
      var ScrollSupply = document.getElementById('ScrollSupply');
      var ScrollSupply1 = document.getElementById('ScrollSupply1');
      var ScrollSupply2 = document.getElementById('ScrollSupply2');
      var speed = 150; //数值越大滚动速度越慢
      var TrLength = $('#ScrollSupply1 tr').length;
      if (TrLength > 6) {
        ScrollSupply2.innerHTML = ScrollSupply1.innerHTML;
        ScrollSupply.scrollTop = ScrollSupply.scrollHeight + 'px';

        function Marquee() {
          //向下的无缝循环滚动
          if (ScrollSupply2.offsetTop - ScrollSupply.scrollTop <= 0)
            //当滚动至demo1与demo2交界时
            ScrollSupply.scrollTop -= ScrollSupply1.offsetHeight;
          //demo跳到最顶端
          else {
            ScrollSupply.scrollTop++;
          }
        }
        var MyMar = setInterval(Marquee, speed);
        ScrollSupply.onmouseover = function () {
          clearInterval(MyMar);
        };
        ScrollSupply.onmouseout = function () {
          MyMar = setInterval(Marquee, speed);
        };
      }
    });
    //供求-求购信息
    $(function () {
      buyInfo(1);
