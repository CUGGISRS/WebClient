                for (var i = 0; i < res.data.rows.length; i++) {
                  var title = res.data.rows[i].title;
                  var time = res.data.rows[i].pub_time.substr(0, 10);

                  var str = '';
                  if (title.length > 18) {
                    str = title.substring(0, 18) + '...';
                  } else {
                    str = title;
                  }
                  html +=
                    '<tr>' +
                    "<td class='title text-left'><a href='#' onclick='tradeDetail(" +
                    res.data.rows[i].id +
                    ")'>" +
                    str +
                    '</a></td>' +
                    "<td class='name'>" +
                    res.data.rows[i].contact_person +
                    '</td>' +
                    "<td class='phone'>" +
                    res.data.rows[i].phone +
