  <script src="./js/commom/api.js"></script>
  <script>
    var url = window.location.search.indexOf('=');
    var id = window.location.search.substr(url + 1);

    $(document).ready(function () {
      $.ajax({
        url: Interface + '/article/id?id=' + id, //请求地址
        dataType: 'json', //数据格式
        type: 'get', //请求方式
        cache: true,
        contentType: 'application/json;charset=utf-8',
        async: false, //是否异步请求
        success: function (data) {
          console.log(data);
          $('.title,.newsTypes').html(data.data.title);
          $('.nav-path').html(data.data.articleSysList[0].articleTypeid);
          $('.author').html('来源：' + data.data.author);
          if (data.data.pubTime != undefined) {
            $('.updatetime').html('发布时间：' + data.data.pubTime.substr(0, 10));
          }
          $('.newsTypes').html(data.data.type);

          $('.content').html(data.data.content);
        },
        error: function (e) {
          console.log('2222');
        },
      });
    });
  </script>
